{
  "compilerOptions": {
    "jsx": "react-jsx",
		"baseUrl": ".",
		"paths": {
      "@/*": ["src/*"],
			"@api/*": ["src/api/*"],
			"@app/*": ["src/app/*"],
			"@components/*": ["src/components/*"],
			"@features/*": ["src/features/*"],
			"@utils/*": ["src/utils/*"],
			"@assets/*": ["src/assets/*"],
      "@theme/*": ["src/theme/*"],
      "@devTools/*": ["src/devTools/*"]
		},
  },
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ]
}
